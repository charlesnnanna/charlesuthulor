  body
    header#header
      .container.main-menu
        .row.align-items-center.justify-content-between.d-flex
          #logo
            a(href="index.html")
              img(src="img/logo.png" alt="" title="")
          nav#nav-menu-container
            ul.nav-menu
              li
                a(href="/") Home
              li
