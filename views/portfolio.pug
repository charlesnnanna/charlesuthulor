              li
                a(href="/portfolio") Portfolio
             
                  
            
          // #nav-menu-container
    // #header
    // start banner Area
    section.about-banner
      .container
        .row.d-flex.align-items-center.justify-content-center
          .about-content.col-lg-12
            h1.text-white
              | Charles' Portfolio
            p.text-white.link-nav
              a(href="index.html") Home 
              span.lnr.lnr-arrow-right
              a(href="/portfolio") Charles' Portfolio
    // End banner Area
    // Start portfolio-area Area
    section#portfolio.portfolio-area.section-gap
      .container
        .row.d-flex.justify-content-center
          .menu-content.pb-70.col-lg-8
            .title.text-center
              h1.mb-10 Our Latest Featured Projects
              p Who are in extremely love with eco friendly system.
        .filters
          ul
            li.active(data-filter="*") All
            li(data-filter=".vector") Vector
            li(data-filter=".raster") Raster
            li(data-filter=".ui") UI/UX
            li(data-filter=".printing") Printing
        .filters-content
